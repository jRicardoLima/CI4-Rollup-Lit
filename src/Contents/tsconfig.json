{
    "compilerOptions": {
        "target": "es2022",
        "module": "esnext",
        "moduleResolution": "bundler",
        "strict": true,
        "skipLibCheck": true,
        "noEmit": true,
        "useDefineForClassFields": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": false,
        "importHelpers": true
    },
    "include": [
        "writable/resources/frontend/**/*.ts"
    ]
}